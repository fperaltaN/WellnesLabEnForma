    <!--TBL_REPORTE_PAGO-->
    <add key="QueryGETReportePago" value="&#xD;&#xA;         SELECT  p.id_pago,             &#xD;&#xA;              p.id_paquete,&#xD;&#xA;              p.id_socio,&#xD;&#xA;              p.ID_USUARIO,         &#xD;&#xA;              s.nombre + ' ' + s.ap_paterno + ' ' + s.ap_materno AS nombre,  &#xD;&#xA;              pq.nombre as paquete,       &#xD;&#xA;                 p.fecha_pago,&#xD;&#xA;              p.fecha_pago_vence,         &#xD;&#xA;              DATEDIFF(month, p.fecha_pago,p.fecha_pago_vence)   As meses,        &#xD;&#xA;              e.nombre + ' ' + e.APELLIDO_PAT + ' ' + e.APELLIDO_MAT AS empleado,         &#xD;&#xA;              p.importe,&#xD;&#xA;              p.pendiente,&#xD;&#xA;              p.activo              &#xD;&#xA;                FROM TblPago as P               &#xD;&#xA;                JOIN TblSocio as S on s.id_socio = p.id_socio              &#xD;&#xA;                 JOIN TblPaquete as Pq on pq.id_paquete = p.id_paquete             &#xD;&#xA;                    JOIN TBL_USUARIOS as e on e.ID_USUARIO = p.ID_USUARIO           &#xD;&#xA;                 where fecha_pago between Cast('{0}'  as date) and Cast('{1}'  as date) &#xD;&#xA;               order by fecha_pago asc&#xD;&#xA;         " />
    <add key="QueryGETTotalReportePago" value="&#xD;&#xA;         SELECT SUM( p.importe) as total    &#xD;&#xA;              FROM TblPago as P               &#xD;&#xA;              JOIN TblSocio as S on s.id_socio = p.id_socio              &#xD;&#xA;               JOIN TblPaquete as Pq on pq.id_paquete = p.id_paquete             &#xD;&#xA;                  JOIN TBL_USUARIOS as e on e.ID_USUARIO = p.ID_USUARIO           &#xD;&#xA;               where fecha_pago between Cast('{0}'  as date) and Cast('{1}'  as date) &#xD;&#xA;             --order by fecha_pago asc&#xD;&#xA;     " />
    <add key="QuerySAVE" value="" />
    <add key="QueryUPDATE" value="" />

