         value="SELECT [id_modulo],[descripcion],[activo],[fecha_modificacion]FROM [EnForma].[dbo].[TblModulo]"/>
    <add key="QuerySAVE" value=""/>
    <add key="QueryUPDATE" value=""/>

    <!--TBL_PAGO modificar fecha pago vence {4}-->
    <add key="QueryGETPago"
      value="SELECT [id_pago],[id_paquete],[id_socio],[ID_USUARIO],[importe],[fecha_pago],[activo],[fecha_modificacion],[fecha_pago_vence],[pendiente]FROM [EnForma].[dbo].[TblPago]"/>
    <add key ="QueryGETSocioPaquete"
         value ="SELECT [nombre],[descripcion],[costo],[id_socio],[id_paquete]FROM [dbo].[SocioPaquete_V] where id_socio={0}"/>
    <add key ="QueryGETPendiente" 
         value="SELECT TOP (1) [fecha_pago_vence],[pendiente]FROM [EnForma].[dbo].[TblPago] where id_socio={0} order by fecha_pago desc;"/>
    <add key="QuerySAVEPago" value="INSERT INTO [dbo].[TblPago]([id_paquete],[id_socio],[ID_USUARIO],[importe],[fecha_pago],[activo],[fecha_modificacion],[fecha_pago_vence],[pendiente])VALUES({0},{1},{2},{3},CURRENT_TIMESTAMP ,1,CURRENT_TIMESTAMP ,CURRENT_TIMESTAMP,{4})"/>
    <add key="QueryUPDATE" value=""/>

    <!--TBL_PAGO_RECARGO-->
    <add key="QueryGETPagoRecargo" value="SELECT [id_pago_recargo],[id_pago],[id_recargo],[activo],[fecha_modificacion]FROM [EnForma].[dbo].[TblPagoRecargo]"/>
    <add key="QuerySAVE" value=""/>
    <add key="QueryUPDATE" value=""/>

    <!--TBL_PAQUETE-->
    <add key="QueryGETPaquete"
      value="SELECT [id_paquete],[nombre],[descripcion],[costo],[ID_USUARIO],[activo],[fecha_modificacion],[diaspaquetes]FROM [EnForma].[dbo].[TblPaquete]"/>
    <add key="QuerySAVEPaquete" value="INSERT INTO [EnForma].[dbo].[TblPaquete](
         [nombre],
         [descripcion],
         [costo],
         [ID_USUARIO],
         [activo],
         [fecha_modificacion],
         [diaspaquetes]
         )VALUES('{0}','{1}',{2},{3},'{4}','{5}',{6});"/>
    <add key="QueryUPDATEPaquete" value="UPDATE [EnForma].[dbo].[TblPaquete]SET 
         [nombre] = '{0}',
         [descripcion] = '{1}',
         [costo] = {2},
         [ID_USUARIO] = {3},
         [diaspaquetes] ={4},
         [activo] = '{5}',
         [fecha_modificacion] = '{6}' 
         WHERE [id_paquete] = {7};"/>
    <add key="QueryGETPaqueteID"
         value="SELECT [id_paquete]
        ,[nombre]
        ,[descripcion]
        ,[costo]
        ,[ID_USUARIO]
        ,[activo]
        ,[fecha_modificacion]
        ,[diaspaquetes]
        FROM [EnForma].[dbo].[TblPaquete]
        WHERE [id_paquete]={0}"/>

