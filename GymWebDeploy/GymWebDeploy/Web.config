    <!--TBL_REPORTE_PAGO-->
    <add key="QueryGETReportePago" value="
         SELECT  p.id_pago,             
		            p.id_paquete,
		            p.id_socio,
		            p.ID_USUARIO,         
		            s.nombre + ' ' + s.ap_paterno + ' ' + s.ap_materno AS nombre,  
		            pq.nombre as paquete,       
	                p.fecha_pago,
		            p.fecha_pago_vence,         
		            DATEDIFF(month, p.fecha_pago,p.fecha_pago_vence)   As meses,        
		            e.nombre + ' ' + e.APELLIDO_PAT + ' ' + e.APELLIDO_MAT AS empleado,         
		            p.importe,
		            p.pendiente,
		            p.activo              
			             FROM TblPago as P               
			             JOIN TblSocio as S on s.id_socio = p.id_socio              
			              JOIN TblPaquete as Pq on pq.id_paquete = p.id_paquete             
		                  JOIN TBL_USUARIOS as e on e.ID_USUARIO = p.ID_USUARIO           
			              where fecha_pago between Cast('{0}'  as date) and Cast('{1}'  as date) 
		             order by fecha_pago asc
         "/>
    <add key="QueryGETTotalReportePago" value="
         SELECT SUM( p.importe) as total    
			           FROM TblPago as P               
			           JOIN TblSocio as S on s.id_socio = p.id_socio              
			            JOIN TblPaquete as Pq on pq.id_paquete = p.id_paquete             
		                JOIN TBL_USUARIOS as e on e.ID_USUARIO = p.ID_USUARIO           
			            where fecha_pago between Cast('{0}'  as date) and Cast('{1}'  as date) 
		           --order by fecha_pago asc
     "/>
    <add key="QuerySAVE" value="" />
    <add key="QueryUPDATE" value="" />

