    <add key ="QueryGETCumple" value="
         SELECT e.[id_socio],
			e.num_socio,
			e.nombre,
			e.ap_paterno, 
			e.ap_materno,
			e.direccion,
			e.mail
				FROM TblSocio As E
				 WHERE DAY(E.fecha_nacimiento)=DAY(GETDATE()) and MONTH(E.fecha_nacimiento)=MONTH(GETDATE())
